                                                                                });
                                                                        }
                                                                });

                                                        }else{

                                                                        //If Callback
                                                                        if(jQuery.inArray("callbl", sact)!==-1){
                                                                                var ybl = $(".yourcallbl").text().split(",");
                                                                                $("#loadingdiv").load("update_cbtime.php", { \'dtp_input1\': ybl[0], \'dtp_input2\': ybl[1], \'eid\': eid }, function(response) {
