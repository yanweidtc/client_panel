                                                                        }else{

                                                                                //Final Refresh
                                                                                $(".pooltablediv").load("maintable.php", { \'sel\': \'go\'}, function() {
                                                                                        var selText = $(".recordsel").attr("id");;
                                                                                        $(".pooltr").hide();
                                                                                        $("."+selText).show();

                                                                                        //rebind the events
                                                                                        searchHide();
                                                                                });

                                                                                $(".actlistdiv").load("cbtable.php", { \'sel\': \'go\'}, function() {
